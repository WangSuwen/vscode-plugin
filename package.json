{
	"name": "vscode-plugin",
	"displayName": "vscode-plugin",
	"publisher": "wxq",
	"description": "",
	"version": "0.0.1",
	"repository": "",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.niHao",
				"title": "NI HAO"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Code插件demo",
			"properties": {
				"vscodePluginDemo.imgPath": {
					"type": "string",
					"default": "",
					"description": "图片地址(文件的磁盘地址)，例子：/Users/xxx/Downloads/demo.jpg"
				},
				"vscodePluginDemo.intervalTime": {
					"type": "string",
					"default": 3600,
					"description": "图片弹出的时间间隔，以 “秒” 为单位"
				},
				"vscodePluginDemo.yourName": {
					"type": "string",
					"default": "guest",
					"description": "姓名"
				},
				"vscodePluginDemo.showTip": {
					"type": "boolean",
					"default": true,
					"description": "启动时显示自定义欢迎页"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test",
		"build": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
