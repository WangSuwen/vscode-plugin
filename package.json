{
	"name": "vscode-plugin",
	"displayName": "love yourself！❤️",
	"publisher": "RX",
	"description": "自定义图片、定时任务间隔",
	"version": "0.0.9",
	"repository": "",
	"engines": {
		"vscode": "^1.10.0"
	},
	"icon": "WX20190520-144711@2x.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.niHao",
				"title": "NI HAO"
			}
		],
		"configuration": {
			"type": "object",
			"title": "亲，保重身体(づ￣3￣)づ╭❤～",
			"properties": {
				"forYourHealth.imgsPath": {
					"type": "array",
					"default": [],
					"description": "图片地址(文件的磁盘地址)，例子：/Users/xxx/Downloads/demo.jpg"
				},
				"forYourHealth.customImgStyle01": {
					"type": "object",
					"default": {},
					"description": "自定义图片的样式表"
				},
				"forYourHealth.intervalTime": {
					"type": "string",
					"default": 3600,
					"description": "图片弹出的时间间隔，以 “秒” 为单位"
				},
				"forYourHealth.yourName": {
					"type": "string",
					"default": "guest",
					"description": "姓名"
				},
				"forYourHealth.customTips01": {
					"type": "string",
					"default": "亲，你已经工作很久了，起来活动一下吧",
					"description": "自定义问候语"
				},
				"forYourHealth.customHiTipsStyle01": {
					"type": "object",
					"default": {
						"display": "block"
					},
					"description": "自定义问候语的样式表"
				},
				"forYourHealth.tabTitle": {
					"type": "string",
					"default": "(づ￣3￣)づ╭❤～亲，保重身体哦",
					"description": "tab页的标题"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test",
		"build": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
